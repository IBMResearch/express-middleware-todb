{
  "name": "jlocke",
  "description": "Express.js analytics for friends.",
  "version": "0.1.1",
  "author": {
    "name": "IBM Research - ACQX Team",
    "email": "ismael.faro1@ibm.com"
  },
  "license": "Apache-2.0",
  "repository": "IBMResearch/jlocke",
  "keywords": [
    "monitoring",
    "metrics",
    "loopback",
    "express",
    "middleware",
    "request",
    "requests",
    "database",
    "IBM",
    "elastic",
    "elasticsearch",
    "kibana"
  ],
  "bugs": {
    "url": "https://github.com/IBMResearch/jlocke/issues"
  },
  "scripts": {
    "start": "node bin",
    "test-unit": "mocha test/unit",
    "test-acceptance": "mocha --timeout 15000 test/acceptance",
    "test": "npm run test-unit && npm run test-acceptance",
    "lint": "eslint . --ignore-path .gitignore",
    "format": "prettier --write '**/*.js'",
    "precommit": "pretty-quick --staged",
    "check-dependencies": "depcheck --ignores='husky'",
    "pretest": "npm run lint && npm run check-dependencies"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "elasticsearch": "^15.0.0",
    "ipaddr.js": "^1.6.0",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "body-parser": "^1.18.2",
    "depcheck": "^0.6.9",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-json": "^1.2.0",
    "express": "^4.16.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.6.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "response-time": "^2.3.2",
    "system-sleep": "^1.3.6"
  },
  "engines": {
    "node": ">=8.0"
  }
}
